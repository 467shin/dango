{
  "name": "@jamashita/dango",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/dango.git"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "type": "module",
  "scripts": {
    "build": "yarn clean && next build",
    "clean": "rimraf out && rimraf .next",
    "dev": "next dev",
    "format": "next lint",
    "prepare": "is-ci || husky install",
    "start": "next start",
    "test": "vitest",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade && yarn audit fix --force",
    "update:upgrade": "ncu --packageManager yarn --deep --upgrade"
  },
  "dependencies": {
    "next": "13.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "10.4.13",
    "husky": "8.0.2",
    "is-ci": "3.0.1",
    "lint-staged": "13.1.0",
    "npm-check-updates": "16.6.2",
    "postcss": "8.4.20",
    "rimraf": "3.0.2",
    "sort-package-json": "2.1.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4",
    "vitest": "0.26.2"
  }
}
